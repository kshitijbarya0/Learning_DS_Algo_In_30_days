int floorSqrt(int n)
{
    // Write your code here.
   int start = 1;
   int end = n;
   int ans = -1;
   while(start<=end){

       int mid = (start+end)/2;
       
       if(mid*mid == n){
           return mid;
       }
       else if( mid*mid<=n){
           ans = mid;
           start = mid + 1;
       }
       if(mid*mid>=n){
           end = mid - 1;
       }else{
           start = mid + 1;
       }

   }
   return ans;
}
